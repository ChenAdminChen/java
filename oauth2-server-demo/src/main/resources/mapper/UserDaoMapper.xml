<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.security.dao.UserDao">

    
        <select id="selectByName" resultType="com.chen.security.entity.User" parameterType="string">
            select * from user
            where username = #{username}
        </select>
</mapper>